type Book{
	bookId:ID!
	bookName:String
	bookDesc:String
	authorName:String
	isbn:String
	price:Float
	page:Int
}

type Query{
	getAllBooks:[Book]
	getBookByBookId(bookId:Float):Book
}

type Mutation{
	createBook(bookDto:BookDTO):Book
	updateBookByBookId(bookId:Float, bookDto:BookDTO):Book
	deleteAllBooks:String
	deleteBookByBookId(bookId:Float):String
}

input BookDTO{
	bookName:String
	bookDesc:String
	authorName:String
	isbn:String
	price:Float
	page:Int
}